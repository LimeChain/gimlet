{
  "name": "gimlet",
  "displayName": "Gimlet",
  "description": "Debug Solana programs easily",
  "publisher": "limechain",
  "icon": "images/logo.png",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Solana LLDB Debugger Path",
      "properties": {
        "solanaDebugger.solanaLldbPath": {
          "type": "string",
          "default": "",
          "description": "Path to the solana-lldb executable"
        }
      }
    },
    "commands": [
      {
        "command": "extension.runAgaveLedgerTool",
        "title": "Run Agave Ledger Tool"
      },
      {
        "command": "extension.runSolanaLLDB",
        "title": "Run Solana LLDB"
      },
      {
        "command": "extension.reRunProcessLaunch",
        "title": "Continue process"
      },
      {
        "command": "extension.runAgaveLedgerToolForBreakpoint",
        "title": "Run Agave Ledger Tool for Breakpoint"
      },
       {
        "command": "extension.runGimletSetup",
        "title": "Run Gimlet Setup"
      }
    ],
    "keybindings": [
      {
        "command": "extension.runSolanaLLDB",
        "key": "f6"
      },
      {
        "command": "extension.reRunProcessLaunch",
        "key": "f7"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for JavaScript'",
    "watch": "echo 'No watch needed for JavaScript'",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "ovsx": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/limechain/gimlet.git"
  }
}
